defaults {
    user_friendly_names yes
    find_multipaths     yes
    path_selector "round-robin 0"
    path_grouping_policy failover
}

multipaths {
  multipath {
    wwid 360000000000000000e00000000010001
    alias c3v500
  }
  multipath {
    wwid 360000000000000000e00000000010002
    alias c3v100
  }
  multipath {
    wwid 360000000000000000e00000000020001
    alias c4v500
  }
  multipath {
    wwid 360000000000000000e00000000020002
    alias c4v100
  }  
}

