$TTL    3600
@       IN      SOA {{ net_local_domain }}.  admin.{{ net_local_domain }}.  (
2021122301       ; Serial
1440     ; Refresh
3600     ; Retry
86400     ; Expire )   ; Negative Cache TTL
3600 )   ; Negative Cache TTL
;
; name servers - NS records
                IN      NS      ns1.{{ net_local_domain }}.
                IN      NS      ns2.{{ net_local_domain }}.
                IN      NS      ns3.{{ net_local_domain }}.
;Core record
                IN      A       {{ keepalived_shared_ip }}
; name servers - A records
ns1             IN      A       {{ net_address_master01 }}
